{
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "routes": [
    { "src": "/sign-in/factor-one", "dest": "/index.html" },
    { "src": "/sign-in/(.*)", "dest": "/index.html" },
    { "src": "/sign-up/(.*)", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
